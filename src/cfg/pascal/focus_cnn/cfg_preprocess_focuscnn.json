{
    "name": "PascalFocusCNNPreprocessing",
    "preprocessor": {
        "type": "CocoFocusCNNPreprocessor",
        "args": {
            "img_in_dir_path": "data/raw/PASCAL/images_train/",
            "ann_file_path": "data/raw/PASCAL/ann_train/annotations_train_processed.json",
            "out_dir_path": "data/processed/PASCAL/PascalFocusCNN/train/",
            "cls_map": {
                "person": 1,
                "car": 2,
                "bicycle": 3
            },
            "max_input_size": [
                640,
                640
            ]
        }
    },
    "preprocess": {
        "verbosity": 1,
        "save_dir": "data/processed/PASCAL/PascalFocusCNN/train/"
    },
    "seed": 42
}